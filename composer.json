{
    "name": "lpmatrix/cartie",
    "description": "A clean shopping cart implementation for Laravel",
    "keywords": [
        "lpmatrix",
        "cartie"
    ],
    "homepage": "https://github.com/lpmatrix/cartie",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sanusi Mubaraq",
            "email": "mubaraqsanusi908@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lpmatrix\\Cartie\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lpmatrix\\Cartie\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lpmatrix\\Cartie\\CartieServiceProvider"
            ],
            "aliases": {
                "Cartie": "Lpmatrix\\Cartie\\CartieFacade"
            }
        }
    },
    "repositories": [
    {
      "type": "path",
      "url": "C:\\Users\\Maryam\\Contacts\\Documents\\cartie"
    }
  ]
}
